{
  "name": "ipfs-interop",
  "version": "7.0.0",
  "description": "Interoperability Tests for IPFS",
  "main": "src/index.js",
  "types": "types/src/index.d.ts",
  "type": "module",
  "files": [
    "*",
    "!**/*.tsbuildinfo"
  ],
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "bin": {
    "ipfs-interop": "bin/ipfs-interop.js"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">6.0.0"
  },
  "scripts": {
    "lint": "aegir lint",
    "build": "aegir build",
    "pretest": "aegir build --esm-tests",
    "test": "cross-env IPFS_REUSEPORT=false aegir test",
    "test:node": "cross-env IPFS_REUSEPORT=false aegir test -t node -f test/node.js",
    "test:browser": "cross-env IPFS_REUSEPORT=false aegir test -t browser -f test/browser.js",
    "dep-check": "aegir dep-check package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/interop.git"
  },
  "keywords": [
    "IPFS"
  ],
  "author": "David Dias <daviddias@ipfs.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ipfs/interop/issues"
  },
  "homepage": "https://github.com/ipfs/interop#readme",
  "browser": {
    "go-ipfs": false
  },
  "dependencies": {
    "aegir": "^35.0.2",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "cross-env": "^7.0.2",
    "delay": "^5.0.0",
    "detect-node": "^2.0.4",
    "ipfs-unixfs": "^6.0.3",
    "ipfsd-ctl": "^10.0.3",
    "ipfs-utils": "^9.0.1",
    "ipns": "^0.15.0",
    "is-ci": "^3.0.0",
    "is-os": "^1.0.1",
    "iso-random-stream": "^2.0.0",
    "it-all": "^1.0.1",
    "it-concat": "^2.0.0",
    "it-drain": "^1.0.0",
    "it-last": "^1.0.1",
    "libp2p-webrtc-star": "^0.23.0",
    "libp2p-websockets": "^0.16.1",
    "multiformats": "^9.3.0",
    "nanoid": "^3.1.10",
    "p-retry": "^4.1.0",
    "peer-id": "^0.15.1",
    "pretty-bytes": "^5.1.0",
    "promisify-es6": "^1.0.3",
    "random-fs": "^1.0.3",
    "readable-stream-buffer-stream": "^1.0.0",
    "rimraf": "^3.0.0",
    "uint8arrays": "^3.0.0",
    "wherearewe": "^1.0.0"
  },
  "contributors": [
    "achingbrain <alex@achingbrain.net>",
    "David Dias <daviddias.p@gmail.com>",
    "Steven Allen <steven@stebalien.com>",
    "Vasco Santos <vasco.santos@moxy.studio>",
    "Alan Shaw <alan.shaw@protocol.ai>",
    "Dmitriy Ryajov <dryajov@gmail.com>",
    "Jacob Heun <jacobheun@gmail.com>",
    "Hugo Dias <hugomrdias@gmail.com>",
    "Hector Sanjuan <code@hector.link>",
    "Pedro Santos <pedro.santos@moxy.studio>",
    "Bryan Stenson <bryan.stenson@gmail.com>",
    "Jonathan <jkrone@vt.edu>",
    "Petar Maymounkov <petarm@gmail.com>",
    "Richard Littauer <richard.littauer@gmail.com>"
  ],
  "devDependencies": {
    "go-ipfs": "^0.9.1",
    "ipfs": "^0.59.0",
    "ipfs-http-client": "^53.0.0"
  }
}
